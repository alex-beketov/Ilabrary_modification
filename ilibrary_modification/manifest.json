{
	"manifest_version": 2,
	"name": "Modification for Ilibrary.ru",
	"description": "This extention modify Ilibrary.ru",
	"version": "0.0.1",
	"icons": {
		"16": "favicon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://ilibrary.ru/*",
				"https://ilibrary.ru/*"
			],
			"js": [
				"index.js"
			]
		}
	],
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "favicon.png"
	}
}